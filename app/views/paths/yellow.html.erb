<body bgcolor="#FFFFA3">

<% if flash.any? %>
  <div class="flash">
    <div class="container">
      <h1 color= "#000"><%= flash[:notice]%></h1>
    </div>
  </div>
<%end%>


<div id="rainbow">

  <ul class="yellow-boxes" id="buttons">
    <%@path_array.each do |paths|%>
      <li>
        <a class="green box gas" <%=link_to "", paths%> </a>
      </li>
    <%end%>
  </ul>
</div>
